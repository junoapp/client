      return 'white';\n      };\n\n      svg\n        .append('g')\n        .selectAll('path')\n        .data(shape['features'])\n        .enter()\n        .append('path')\n        .attr('d', path)\n        .style('fill', (d) => accessor(d))\n        .style('stroke', '#ccc')\n        .attr('alt', (d) => d['properties'].name);\n\n      return () => {\n        svg.remove();\n      };\n    });\n  }, [id, props.data]);\n\n  return (\n    <div>\n      <h1>{props.name}</h1>\n      <div id={id}></div>\n    </div>\n  );\n}\n\nexport default MapChart;\n","/Users/paulomenezes/repositories/juno/client/src/pages/Preferences.tsx",["170"],"/Users/paulomenezes/repositories/juno/client/src/components/form/Checkbox.tsx",[],"/Users/paulomenezes/repositories/juno/client/src/charts/GroupedHorizontalBarChart.tsx",[],{"ruleId":"171","replacedBy":"172"},{"ruleId":"173","replacedBy":"174"},{"ruleId":"175","severity":1,"message":"176","line":6,"column":47,"nodeType":"177","messageId":"178","endLine":6,"endColumn":65},{"ruleId":"179","severity":1,"message":"176","line":6,"column":47,"nodeType":"177","messageId":"178","endLine":6,"endColumn":65},{"ruleId":"175","severity":1,"message":"180","line":17,"column":11,"nodeType":"177","messageId":"178","endLine":17,"endColumn":13},{"ruleId":"179","severity":1,"message":"180","line":17,"column":11,"nodeType":"177","messageId":"178","endLine":17,"endColumn":13},{"ruleId":"175","severity":1,"message":"181","line":46,"column":9,"nodeType":"177","messageId":"178","endLine":46,"endColumn":24},{"ruleId":"179","severity":1,"message":"181","line":46,"column":9,"nodeType":"177","messageId":"178","endLine":46,"endColumn":24},{"ruleId":"171","replacedBy":"182"},{"ruleId":"173","replacedBy":"183"},{"ruleId":"175","severity":1,"message":"184","line":6,"column":10,"nodeType":"177","messageId":"178","endLine":6,"endColumn":26},{"ruleId":"179","severity":1,"message":"184","line":6,"column":10,"nodeType":"177","messageId":"178","endLine":6,"endColumn":26},{"ruleId":"